{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "isAdmin"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "title": "The username Schema",
            "examples": [
                "testuser"
            ]
        },
        "password": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "title": "The password Schema",
            "examples": [
                "testpw"
            ]
        },
        "firstName": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "FirstName"
            ]
        },
        "lastName": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "LastName"
            ]
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema",
            "examples": [
                false
            ]
        }
    },
    "examples": [{
        "username": "testuser",
        "password": "testpw",
        "firstName": "FirstName",
        "lastName": "LastName",
        "isAdmin": false
    }]
}
